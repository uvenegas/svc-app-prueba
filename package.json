{
    "name": "svc-config-channel",
    "version": "1.0.0",
    "description": "servicio validacion version",
    "module": "CommonJS",
    "main": "app.js",
    "scripts": {
        "test": "npx tsc && jest",
        "test:watch": "npx jest --watchAll",
        "tsc": "./node_modules/typescript/bin/tsc",
        "start": "node ./dist/src/main.js",
        "start:app": "npm run buildonly && npm run copyConfig && node ./dist/src/main.js",
        "onlystart": "node dist/src/main.js",
        "start:dev": "nodemon",
        "buildonly": "npm run tsc",
        "build": "npm run buildonly && npm run copyConfig",
        "copyConfig": "cp -r ./src/config/* ./dist/src/config",
        "lint": "eslint --ext .ts src",
        "lint:fix": "npm run prettier:fix && eslint --ext .ts src --fix",
        "prettier": "prettier --config .prettierrc.json --check src",
        "prettier:fix": "prettier --config .prettierrc.json --write src",
        "init:app": " npm run tsc && npm run copyConfig && npm run start",
        "prepare": "husky install"
    },
    "author": "Ulises",
    "dependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/node-mailjet": "^3.3.7",
        "@types/node-schedule": "^1.3.1",
        "@types/uuid": "^8.3.0",
        "ajv": "^7.2.3",
        "ajv-errors": "^2.0.1",
        "ajv-formats": "^1.5.1",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "contentful": "^8.2.1",
        "cors": "^2.8.5",
        "dotenv": "^8.6.0",
        "eslint-config-google": "^0.13.0",
        "express": "^4.17.1",
        "express-validator": "^6.14.0",
        "fast-xml-parser": "^3.20.3",
        "form-data": "^4.0.0",
        "husky": "^5.0.9",
        "json-rules-engine": "^6.1.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "log4js": "^6.3.0",
        "module-alias": "^2.2.2",
        "moment": "^2.29.1",
        "mongoose": "^6.5.1",
        "node-schedule": "^2.0.0",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "xmlbuilder2": "^3.0.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.24",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/lodash": "^4.14.175",
        "@types/mocha": "^9.0.0",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^14.17.9",
        "@types/node-mailjet": "^3.3.7",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "eslint": "^7.21.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.1.0",
        "husky": "^5.1.3",
        "jest": "^26.6.3",
        "nock": "^13.0.7",
        "node-mocks-http": "^1.10.1",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.1",
        "ts-node": "^9.1.1",
        "typescript": "4.6.2"
    },
    "engines": {
        "node": "14.15.5"
    }
}
